{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-first1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureBlobStorage2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage2'"
		},
		"AzureBlobStorage3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage3'"
		},
		"AzureBlobStoragelookup_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStoragelookup'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2megha_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2megha'"
		},
		"adlsmovedelete_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsmovedelete'"
		},
		"adlsmovedelete1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsmovedelete1'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adffirst3.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2megha_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firstadlsv2.dfs.core.windows.net/"
		},
		"adlsmovedelete_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firstadlsv2.dfs.core.windows.net/"
		},
		"adlsmovedelete1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firstadlsv2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-first1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureBlobStorage2_connectionString": {
						"value": "[parameters('AzureBlobStorage2_connectionString')]"
					},
					"AzureBlobStorage3_connectionString": {
						"value": "[parameters('AzureBlobStorage3_connectionString')]"
					},
					"AzureBlobStoragelookup_connectionString": {
						"value": "[parameters('AzureBlobStoragelookup_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2megha_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2megha_accountKey')]"
					},
					"adlsmovedelete_accountKey": {
						"value": "[parameters('adlsmovedelete_accountKey')]"
					},
					"adlsmovedelete1_accountKey": {
						"value": "[parameters('adlsmovedelete1_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2megha_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2megha_properties_typeProperties_url')]"
					},
					"adlsmovedelete_properties_typeProperties_url": {
						"value": "[parameters('adlsmovedelete_properties_typeProperties_url')]"
					},
					"adlsmovedelete1_properties_typeProperties_url": {
						"value": "[parameters('adlsmovedelete1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-first1_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-first1_ArmTemplate_0')]"
			]
		}
	]
}